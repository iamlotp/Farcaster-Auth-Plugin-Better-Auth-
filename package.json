{
  "name": "better-auth-farcaster-plugin",
  "version": "2.1.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "./dist/index.d.ts"
      ],
      "client": [
        "./dist/client.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ],
      "react": [
        "./dist/react/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start": "node dist/index.js"
  },
  "keywords": [
    "better-auth farcaster plugin",
    "better-auth farcaster",
    "farcaster authentication",
    "farcaster react hooks"
  ],
  "author": "RezB",
  "license": "ISC",
  "description": "A community-made plug-in to authenticate Farcaster users with optional React hooks",
  "dependencies": {
    "@farcaster/quick-auth": "^0.0.8"
  },
  "peerDependencies": {
    "better-auth": ">=1.2.0",
    "react": ">=17.0.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "better-auth": {
      "optional": false
    },
    "zod": {
      "optional": false
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/bun": "latest",
    "@types/react": "^18.2.0",
    "better-auth": "^1.2.8",
    "nodemon": "^3.1.10",
    "npm-check-updates": "^18.0.1",
    "react": "^18.2.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "zod": "^3.23.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamlotp/Farcaster-Auth-Plugin-Better-Auth-.git"
  }
}